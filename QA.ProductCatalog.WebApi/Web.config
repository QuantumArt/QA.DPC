    <alias alias="IArticleFormatter" type="QA.ProductCatalog.Infrastructure.IArticleFormatter, QA.ProductCatalog.Infrastructure" />
    <alias alias="XmlProductFormatter" type="QA.Core.DPC.Formatters.Services.XmlProductFormatter, QA.Core.DPC.Formatters" />
    <alias alias="JsonProductFormatter" type="QA.Core.DPC.Formatters.Services.JsonProductFormatter, QA.Core.DPC.Formatters" />
    <alias alias="RemoteValidator" type="QA.Validation.Xaml.Extensions.Rules.ProcessRemoteValidationIf, QA.Validation.Xaml.Extensions" />
    <container name="Default">
      <!-- Регистрируем сервис журналирования как синглетон -->
      <register type="ILogger" mapTo="NLogLogger">
        <lifetime type="singleton" />
        <constructor>
          <!-- в конструктор передаем имя файла с описанием правил журналирования -->
          <param name="fileName" value="NLogClient.config" />
        </constructor>
      </register>
      <register type="ILogger" mapTo="NLogLogger" name="Client">
        <lifetime type="singleton" />
        <constructor>
          <!-- в конструктор передаем имя файла с описанием правил журналирования -->
          <param name="fileName" value="NLogClient.config" />
        </constructor>
      </register>
      <register type="ILogger" mapTo="NLogLogger" name="Server">
        <lifetime type="singleton" />
        <constructor>
          <!-- в конструктор передаем имя файла с описанием правил журналирования -->
          <param name="fileName" value="NLogClient.config" />
        </constructor>
      </register>
      <register type="RemoteValidator" mapTo="RemoteValidator">
        <lifetime type="singleton" />
      </register>
      <register type="ISettingsService" mapTo="SettingsFromQpService" />
      <register type="IArticleFormatter" mapTo="XmlProductFormatter" />
    </container>
  </unity>
  <connectionStrings>
    <add name="qp_database" connectionString="Application Name=DPC_Api;Initial Catalog=catalog;Data Source=dbserver;User ID=user;Password=pass;" providerName="System.Data.SqlClient" />
    <add name="consumer_monitoring" connectionString="Application Name=DPC_Api;Initial Catalog=ref_front;Data Source=dbserver;User ID=user;Password=pass;" providerName="System.Data.SqlClient" />
    <add name="consumer_monitoringStage" connectionString="Application Name=DPC_Api;Initial Catalog=ref_stage_front;Data Source=dbserver;User ID=user;Password=pass;" providerName="System.Data.SqlClient" />
  </connectionStrings>
  <appSettings>
    <add key="UserId" value="1" />
  </appSettings>
