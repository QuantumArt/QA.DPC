  <unity>
    <alias alias="ILogger" type="QA.Core.ILogger, QA.Core" />
    <alias alias="NLogLogger" type="QA.Core.NLogLogger, QA.Core" />
    <alias alias="SettingsFromQpService" type="QA.Core.DPC.Loader.SettingsFromQpService, QA.Core.DPC.Loader" />
    <alias alias="SettingsFromContentService" type="QA.Core.DPC.Loader.SettingsFromContentService, QA.Core.DPC.Loader" />
