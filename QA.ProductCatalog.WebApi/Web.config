        <binding name="BasicHttpBinding_INotificationService" allowCookies="true" maxReceivedMessageSize="20000000" maxBufferSize="20000000" maxBufferPoolSize="20000000">
          <readerQuotas maxDepth="32" maxArrayLength="200000000" maxStringContentLength="200000000" />
        </binding>
      </basicHttpBinding>
    </bindings>
    <client>
      <endpoint address="http://mscservices01:8010/DpcNotificationService" binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_INotificationService" contract="Notifications.INotificationService" name="BasicHttpBinding_INotificationService" />
    </client>
  </system.serviceModel>
  <system.webServer>
    <modules>
