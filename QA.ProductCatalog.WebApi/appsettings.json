{
    "Properties": {
        "UseAuthorization": false,
        "Name" : "DPC.WebAPI",
        "Version": "#{GitVersion.Tag}#",
        "BuildVersion": "#{GitVersion.Tag}#.#{GitVersion.CommitsSinceTag}#-#{GitVersion.Sha}#",
        "DocumentLicenceKey": "FREE-LIMITED-KEY"
    },
    
    "Connection": {
        "QpMode": true,
        "UsePostgres": false,
        "TransactionTimeout": "00:10:00"
    },

    "Logging": {
        "IncludeScopes": false,
        "LogLevel": {
            "Default": "Information",
            "Microsoft": "Warning",
            "System": "Warning"
        }
    },
    "Loader":
    {
        "UseFileSizeService": true
    },
    "TMForum": {
        "IsEnabled": true
    }
}