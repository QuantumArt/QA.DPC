    <compilation debug="true" targetFramework="4.7.1"/>
    <httpRuntime targetFramework="4.7"/>
    <authentication mode="None"/>
  </system.web>
  <unity>
    <alias alias="ILogger" type="QA.Core.Logger.ILogger, QA.Core"/>
    <alias alias="NLogLogger" type="QA.Core.Logger.NLogLogger, QA.Core"/>
    <container name="Default">
      <!-- Регистрируем сервис журналирования как синглетон -->
      <register type="ILogger" mapTo="NLogLogger">
        <lifetime type="singleton"/>
        <constructor>
          <!-- в конструктор передаем имя файла с описанием правил журналирования -->
          <param name="fileName" value="NLogClient.config"/>
        </constructor>
      </register>
      <register type="ILogger" mapTo="NLogLogger" name="Client">
        <lifetime type="singleton"/>
        <constructor>
          <!-- в конструктор передаем имя файла с описанием правил журналирования -->
          <param name="fileName" value="NLogClient.config"/>
        </constructor>
      </register>
      <register type="ILogger" mapTo="NLogLogger" name="Server">
        <lifetime type="singleton"/>
        <constructor>
          <!-- в конструктор передаем имя файла с описанием правил журналирования -->
          <param name="fileName" value="NLogClient.config"/>
        </constructor>
      </register>
    </container>
  </unity>
  <system.webServer>
    <validation validateIntegratedModeConfiguration="false"/>
    
  <handlers>
      <remove name="ExtensionlessUrlHandler-Integrated-4.0"/>
      <remove name="OPTIONSVerbHandler"/>
      <remove name="TRACEVerbHandler"/>
      <add name="ExtensionlessUrlHandler-Integrated-4.0" path="*." verb="*" type="System.Web.Handlers.TransferRequestHandler" preCondition="integratedMode,runtimeVersionv4.0"/>
