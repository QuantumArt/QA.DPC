{
  "SonicElasticStore": {
    "DefaultSize": 10,
    "MaxResultWindow": 20000,
    "TotalFieldsLimit": 100000,
    "IdPath": "Id",
    "TypePath": "Type",
    "DefaultType": "untyped",
    "DefaultFields": [
      "Id",
      "Type",
      "ForisID",
      "Regions.Id",
      "Regions.Title",
      "Regions.Alias",
      "MarketingProduct.Id",
      "MarketingProduct.Alias",
      "MarketingProduct.Title",
      "Modifiers.Alias"
    ],

    "IndexingOptions": [
      {
        "Name": "ParamsDic",
        "Path": "Parameters",
        "Keys": [
          "BaseParameter.Alias",
          "BaseParameterModifiers[*].Alias"
        ]
      }
    ],
    "Types": [
      "_default_"
    ],
    "EdgeNgramOptions": {
      "MinNgram": 3,
      "MaxNgram": 20
    },
    "NotAnalyzedFields": [
      "GlobalCode",
      "Alias",
      "ForisID"
    ],
    "DynamicDateFormats": {
      "Default": [
        "MM/dd/yyyy",
        "MM/dd/yyyy HH:mm",
        "MM/dd/yyyy HH:mm:ss",
        "dd.MM.yyyy",
        "dd.MM.yyyy HH:mm",
        "dd.MM.yyyy HH:mm:ss",
        "yyyy-MM-dd",
        "yyyy-MM-dd'T'HH:mm",
        "yyyy-MM-dd'T'HH:mm:ss",
        "yyyy-MM-dd'T'HH:mm:ssZ",
        "yyyy-MM-dd'T'HH:mm:ss.SSS||yyyy-MM-dd'T'HH:mm:ss.SSSZ"
      ],
      "Elastic8": [
        "MM/dd/yyyy",
        "MM/dd/yyyy HH:mm",
        "MM/dd/yyyy HH:mm:ss",
        "dd.MM.yyyy",
        "dd.MM.yyyy HH:mm",
        "dd.MM.yyyy HH:mm:ss",
        "yyyy-MM-dd",
        "yyyy-MM-dd'T'HH:mm",
        "yyyy-MM-dd'T'HH:mm:ss",
        "yyyy-MM-dd'T'HH:mm:ssX",
        "yyyy-MM-dd'T'HH:mm:ss.S||yyyy-MM-dd'T'HH:mm:ss.SS||yyyy-MM-dd'T'HH:mm:ss.SSS",
        "yyyy-MM-dd'T'HH:mm:ss.SX||yyyy-MM-dd'T'HH:mm:ss.SSX||yyyy-MM-dd'T'HH:mm:ss.SSSX"
      ]
    },
    "CreationDateField": "UpdateDate",
    "ValueSeparator": ",",
    "NegationMark": "!",
    "WildcardStarMark": "*",
    "WildcardQuestionMark": "?"
  },

  "Harvester": {
    "ChunkSize": 100
  },

  "Data": {
    "QpMode": true,
    "CanUpdate": true,
    "InstanceId": "demo",
    "VersionCacheExpiration": "00:05:00.000",
    "SyncName": "DPC.Sync",
    "SearchName": "DPC.Search",
    "Version": "#{GitVersion.Tag}#",
    "BuildVersion": "#{GitVersion.Tag}#.#{GitVersion.CommitsSinceTag}#-#{GitVersion.Sha}#"
  },

  "Integration":
  {
    "ConfigurationServiceUrl": "",
    "ConfigurationServiceToken": ""
  },

  "ApiRestrictions": {
    "MaxExpandDepth": 3
  },

  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Information",
      "System": "Warning",
      "Microsoft": "Warning"
    }
  }
}

