{
  "SonicElasticStore": {
    "DefaultSize": 10,
    "MaxResultWindow": 20000,
    "TotalFieldsLimit": 100000,
    "IdPath": "Id",
    "TypePath": "Type",
    "DefaultType": "untyped",
    "DefaultFields": [
      "Id",
      "Type",
      "ForisID",
      "Regions.Id",
      "Regions.Title",
      "Regions.Alias",
      "MarketingProduct.Id",
      "MarketingProduct.Alias",
      "MarketingProduct.Title",
      "Modifiers.Alias"
    ],

    "IndexingOptions": [
      {
        "Name": "ParamsDic",
        "Path": "Parameters",
        "Keys": [
          "BaseParameter.Alias",
          "BaseParameterModifiers[*].Alias"
        ]
      }
    ],

    "Types": [
      "_default_"
    ],
    "NotAnalyzedFields": [
      "GlobalCode",
      "Alias",
      "ForisID"
    ],
    "DynamicDateFormats": [
      "MM/dd/yyyy",
      "MM/dd/yyyy HH:mm",
      "MM/dd/yyyy HH:mm:ss",
      "dd.MM.yyyy",
      "dd.MM.yyyy HH:mm",
      "dd.MM.yyyy HH:mm:ss",
      "yyyy-MM-dd",
      "yyyy-MM-dd'T'HH:mm",
      "yyyy-MM-dd'T'HH:mm:ss",
      "yyyy-MM-dd'T'HH:mm:ssZ",
      "yyyy-MM-dd'T'HH:mm:ss.SSS",
      "yyyy-MM-dd'T'HH:mm:ss.SSSZ"
    ],
    "CreationDateField": "UpdateDate",
    "ValueSeparator": ",",
    "NegationMark": "!",
    "WildcardStarMark": "*",
    "WildcardQuestionMark": "?"
  },

  "Harvester": {
    "ChunkSize": 100
  },

  "Data": {
    "CanUpdate": true,
    "VersionCaceExpiration": "00:05:00.000"
  },


  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Information"
    }
  }
}

