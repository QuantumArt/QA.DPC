<configuration>
  <configSections>
    <section name="unity" type="Microsoft.Practices.Unity.Configuration.UnityConfigurationSection, Unity.Configuration" />
    <section name="ConfigurationStorage" type="QA.Configuration.XamlConfigurationSection, QA.Configuration" />
    
    <section name="entityFramework" type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=5.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
  <!-- For more information on Entity Framework configuration, visit http://go.microsoft.com/fwlink/?LinkID=237468 --></configSections>
  <connectionStrings>
    <add name="QA.Core.DPC.Integration.Properties.Settings.dpc_webConnectionString" connectionString="Data Source=mscsql01;Initial Catalog=dpc_web;Integrated Security=True" providerName="System.Data.SqlClient" />
  </connectionStrings>
  <appSettings>
    <add key="webpages:Version" value="2.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="PreserveLoginUrl" value="true" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
  </appSettings>
  <unity xmlns="http://schemas.microsoft.com/practices/2010/unity">
    <!-- описываем интерфейс сервиса журналирования -->
    <alias alias="ILogger" type="QA.Core.Logger.ILogger, QA.Core" />
    <!-- описываем конкретные классы журналирования -->
    <alias alias="NLogLogger" type="QA.Core.Logger.NLogLogger, QA.Core" />
    <!--описываем интерфейс и реализацию универсального конфигуратора-->
    <alias alias="IConfigurationService" type="QA.Configuration.IConfigurationService, QA.Configuration" />
    <alias alias="ConfigurationService" type="QA.Configuration.ConfigurationService, QA.Configuration" />
    <alias alias="IDpcProductService" type="QA.Core.DPC.Integration.IDpcProductService, QA.Core.DPC.Integration" />
    <alias alias="DpcProductService" type="QA.Core.DPC.Integration.DpcProductService, QA.Core.DPC.Integration" />
    <alias alias="ICacheProvider" type="QA.Core.ICacheProvider, QA.Core" />
    <alias alias="CacheProvider" type="QA.Core.CacheProvider, QA.Core" />
    <alias alias="IProductSerializer" type="QA.Core.DPC.Integration.IProductSerializer, QA.Core.DPC.Integration" />
    <alias alias="XmlProductSerializer" type="QA.Core.DPC.Integration.XmlProductSerializer, QA.Core.DPC.Integration" />
    <alias alias="JsonProductSerializer" type="QA.Core.DPC.Integration.JsonProductSerializer, QA.Core.DPC.Integration" />
    <!--регистрируем unitycontainer c именем Default-->
    <container name="Default">
      <!-- Регистрируем сервис журналирования как синглетон -->
      <register type="ILogger" mapTo="NLogLogger">
        <lifetime type="singleton" />
        <constructor>
          <!-- в конструктор передаем имя файла с описанием правил журналирования -->
          <param name="fileName" value="NLogClient.config" />
        </constructor>
      </register>
      <!-- регистрируем конфигуратор -->
      <register type="IConfigurationService" mapTo="ConfigurationService">
        <lifetime type="singleton" />
      </register>
      <register type="IDpcProductService" mapTo="DpcProductService">
        <lifetime type="singleton" />
      </register>
      <!--регистрируем кеш-провайдер как синглтон-->
      <register type="ICacheProvider" mapTo="CacheProvider">
        <lifetime type="singleton" />
      </register>
      <register type="IProductSerializer" mapTo="XmlProductSerializer" />
    </container>
  </unity>
  <ConfigurationStorage xmlns="http://artq.com/configuration" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" />
  <!--
    For a description of web.config changes see http://go.microsoft.com/fwlink/?LinkId=235367.

    The following attributes can be set on the <httpRuntime> tag.
      <system.Web>
        <httpRuntime targetFramework="4.5.2" />
      </system.Web>
  -->
  <system.web>
    <httpRuntime targetFramework="4.5" />
    <compilation debug="true" targetFramework="4.5.2" />
    <pages>
      <namespaces>
        <add namespace="System.Web.Helpers" />
        <add namespace="System.Web.Mvc" />
        <add namespace="System.Web.Mvc.Ajax" />
        <add namespace="System.Web.Mvc.Html" />
        <add namespace="System.Web.Routing" />
        <add namespace="System.Web.WebPages" />
      </namespaces>
    </pages>
  </system.web>
  <system.webServer>
    <validation validateIntegratedModeConfiguration="false" />
    <modules>
      <remove name="WebDAVModule" />
    </modules>
    <handlers>
      <remove name="WebDAV" />
      
      
      
      
    <remove name="ExtensionlessUrlHandler-Integrated-4.0" /><remove name="OPTIONSVerbHandler" /><remove name="TRACEVerbHandler" /><add name="ExtensionlessUrlHandler-Integrated-4.0" path="*." verb="*" type="System.Web.Handlers.TransferRequestHandler" preCondition="integratedMode,runtimeVersionv4.0" /></handlers>
  </system.webServer>
  <system.serviceModel>
    <serviceHostingEnvironment aspNetCompatibilityEnabled="true" multipleSiteBindingsEnabled="true">
      <serviceActivations>
        <!--Регистрируем адрес сервиса -->
        <add relativeAddress="DpcService.svc" service="QA.Core.DPC.Integration.DpcProductService" factory="QA.ProductCatalog.SiteSyncWebHost.Services.UnityServiceHostFactory" />
      </serviceActivations>
    </serviceHostingEnvironment>
    <services>
      <service name="QA.Core.DPC.Integration.DpcProductService">
        <endpoint address="" binding="basicHttpBinding" contract="QA.Core.DPC.Integration.IDpcService">
          <identity>
            <dns value="mscservices01" />
          </identity>
        </endpoint>
        <endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange" />
      </service>
    </services>
    <behaviors>
      <serviceBehaviors>
        <behavior>
          <!-- To avoid disclosing metadata information, 
          set the values below to false before deployment -->
          <serviceMetadata httpGetEnabled="True" httpsGetEnabled="True" />
          <!-- To receive exception details in faults for debugging purposes, 
          set the value below to true.  Set to false before deployment 
          to avoid disclosing exception information -->
          <serviceDebug includeExceptionDetailInFaults="False" />
        </behavior>
      </serviceBehaviors>
    </behaviors>
  </system.serviceModel>
