@using QA.ProductCatalog.Admin.WebApp.Core
@model QA.Core.DPC.UI.Controls.Template
@if (Model == null || Model.Hidden) { return; }
@if (Model.Content != null)
{
    Model.DataContext = (object)Model.Items ?? (object)Model.CurrentItem;        
    @Html.DisplayFor(x => Model.Content)
}
else
{
    @Html.DisplayForUIElement(Model.CellTemplate)
}

